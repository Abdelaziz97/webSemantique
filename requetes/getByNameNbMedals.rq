SELECT ?human ?count 
WHERE{
	SELECT ?human (count(*) as ?count) 
	WHERE {?event rdf:type dbo:OlympicEvent{?event dbo:bronzeMedalist ?human .  } 
	UNION {?event dbo:silverMedalist ?human} 
	UNION {?event dbo:goldMedalist ?human}
	FILTER regex(?human, "Michael_Phelps")}
	GROUP BY ?human}
ORDER BY DESC(?count)
