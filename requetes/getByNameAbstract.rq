SELECT ?image
WHERE{
	SELECT ?image
	WHERE {?event rdf:type dbo:OlympicEvent{?event dbo:bronzeMedalist ?human. ?human dbo:abstract ?image } 
	UNION {?event dbo:silverMedalist ?human . ?human dbo:abstract ?image } 
	UNION {?event dbo:goldMedalist ?human . ?human dbo:abstract ?image }
	FILTER regex(?human, "Michael_Phelps")
        FILTER (lang(?image) = 'en')}
	GROUP BY ?human}
